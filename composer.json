{
    "name": "ilaravel/core",
    "description": "iLaravel Core",
    "keywords": ["laravel","manage"],
    "homepage": "https://ilaravel.iamir.net",
    "license": "MIT",
    "authors": [
        {
            "name" : "iamirnet",
            "email" : "me@iamir.net",
            "homepage": "https://iamir.net",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "App\\": "src/app",
            "iLaravel\\Core\\": "src/",
            "iLaravel\\Core\\iApp\\Database\\Query\\": "src/iapp/Database/Query"
        },
        "files": [
            "src/helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "iLaravel\\Core\\Providers\\AppServiceProvider",
                "iLaravel\\Core\\Providers\\RouteServiceProvider",
                "iLaravel\\Core\\Providers\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "iamirnet/curl": "*",
        "laravel/socialite": "*",
        "laravel/passport": "*",
        "lcobucci/jwt": "*",
        "intervention/image": "*",
        "doctrine/dbal": "*"
    }
}
